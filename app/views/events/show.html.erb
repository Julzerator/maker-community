<div class="theme-program">
  <div class="container">
    <div class="events-show">
      <div class="main">
        <h1><%= @event_session.event.title %></h1>
        <% if @event_session.event.image.present? %> <%= image_model_tag(@event_session.event.image, class: 'event-image') %> <% end %>

        <div><%= render_html_without_line_breaks @event_session.event.content %></div>
      </div>
      <div class="details">details</div>
    </div>
  </div>
</div>
