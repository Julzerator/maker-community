{
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "files.associations": {
    "*.html.erb": "erb"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "ruby.rubocop.useBundler": true,
  "[ruby]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html.erb]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true,
    "app/assets/builds/*.*": true,
    "tmp": true,
    "log": true
  },
  "explorerExclude.backup": null
}
